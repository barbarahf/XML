<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE persona [
        <!ELEMENT persona (nombre, mujer, fecha_de_nacimiento, ciudad)>
        <!ELEMENT fecha_de_nacimiento EMPTY>
        <!ELEMENT nombre (#PCDATA)>
        <!ELEMENT mujer (#PCDATA)>
        <!ATTLIST fecha_de_nacimiento dia CDATA #REQUIRED>
        <!ATTLIST fecha_de_nacimiento  mes CDATA #REQUIRED>
        <!ATTLIST fecha_de_nacimiento anyo CDATA "2000">
        <!ELEMENT ciudad (#PCDATA)>
        ]>
<!-- DTD ATRRIBUTOS
-->
<persona>
    <nombre>Maria</nombre>
    <mujer/>
    <fecha_de_nacimiento dia="21" mes="11" anyo="2000"/>
    <ciudad>Bilbao</ciudad>
</persona>